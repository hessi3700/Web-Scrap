# Cloudflare Worker ingest endpoint (POST scraped data here)
API_INGEST_URL=https://your-worker.workers.dev/api/ingest
API_INGEST_SECRET=your-secret-header-value

# Scraper config (optional)
SCRAPE_DELAY_SECONDS=2
USER_AGENT=WebScrapBot/1.0 (+https://github.com/yourusername/web-scrap)
